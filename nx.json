{
  "npmScope": "base11",
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "my-react-app": {
      "tags": []
    },
    "my-react-app-e2e": {
      "tags": [],
      "implicitDependencies": ["my-react-app"]
    },
    "shared-components": {
      "tags": []
    },
    "app-domain-1": {
      "tags": []
    },
    "app-domain-2": {
      "tags": []
    }
  }
}
